{
  "name": "js-dev-environment",
  "version": "1.0.0",
  "description": "Js dev environment based upon Pluralsight course by Cory House",
  "scripts": {
    "prestart": "babel-node scripts/startupMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch start-mock-api",
    "lint": "esw webpack.config.* src scripts --color",
    "lint:watch": "npm run lint -- --watch",
    "open:src": "babel-node scripts/server.js",
    "security-check": "nsp check",
    "test": "mocha --reporter progress scripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node scripts/genMockData",
    "prestart-mock-api": "npm run generate-mock-data",
    "start-mock-api": "json-server --watch src/api/db.json --port 8089",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node scripts/build.js",
    "postbuild": "babel-node scripts/distServer.js"
  },
  "author": "Johannes Lindgren",
  "license": "MIT",
  "dependencies": {
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "6.2.5",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chalk": "2.3.2",
    "cheerio": "1.0.0-rc.2",
    "compression": "1.7.2",
    "cross-env": "5.1.3",
    "css-loader": "0.25.0",
    "eslint": "4.18.2",
    "eslint-plugin-import": "2.9.0",
    "eslint-watch": "3.1.3",
    "express": "4.16.2",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.22.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "0.3.6",
    "json-server": "0.8.22",
    "mocha": "5.0.4",
    "nock": "8.1.0",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "open": "0.0.5",
    "rimraf": "2.6.2",
    "style-loader": "0.13.1",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  }
}
