{
  "name": "funcs",
  "version": "1.0.0",
  "description": "A library of functions",
  "scripts": {
    "prestart": "babel-node scripts/startupMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch start-mock-api",
    "lint": "esw src --color",
    "lint:watch": "npm run lint -- --watch",
    "open:src": "babel-node scripts/server.js",
    "test": "mocha --reporter progress scripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node scripts/genMockData",
    "prestart-mock-api": "npm run generate-mock-data",
    "start-mock-api": "json-server --watch src/api/db.json --port 8089",
    "prebuild": "npm-run-all test lint",
    "build": "babel-node scripts/build.js",
    "postbuild": "babel-node scripts/distServer.js"
  },
  "author": "Johannes Lindgren",
  "license": "MIT",
  "dependencies": {
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chalk": "^4.0.0",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-watch": "^6.0.1",
    "express": "^4.17.1",
    "mini-css-extract-plugin": "^0.9.0",
    "html-webpack-plugin": "^4.0.4",
    "jsdom": "^16.2.2",
    "json-schema-faker": "^0.5.0-rcv.24",
    "json-server": "^0.16.1",
    "mocha": "^7.1.1",
    "nock": "^12.0.3",
    "npm-run-all": "^4.1.5",
    "open": "^7.0.3",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-clean-plugin": "^0.2.3",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}
