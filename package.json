{
  "name": "funcs",
  "version": "1.0.0",
  "description": "A library of functions",
  "scripts": {
    "start": "npm-run-all --parallel open:src lint:watch test:watch start-mock-api",
    "lint": "esw src --color",
    "lint:watch": "npm run lint -- --watch",
    "open:src": "babel-node scripts/server.js",
    "test": "mocha --reporter progress scripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node scripts/genMockData",
    "prestart-mock-api": "npm run generate-mock-data",
    "start-mock-api": "json-server --watch src/api/db.json --port 8089",
    "prebuild": "npm-run-all test lint",
    "build": "babel-node scripts/build.js",
    "postbuild": "babel-node scripts/distServer.js"
  },
  "author": "Johannes Lindgren",
  "license": "The MIT License (MIT)",
  "dependencies": {
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@babel/node": "^7.18.10",
    "@babel/preset-env": "^7.19.0",
    "@babel/register": "^7.18.9",
    "babel-loader": "^8.2.5",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.10",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.23.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-watch": "^8.0.0",
    "express": "^4.17.2",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^20.0.0",
    "json-schema-faker": "^0.5.0-rcv.41",
    "json-server": "^0.17.0",
    "mini-css-extract-plugin": "^2.6.1",
    "mocha": "^10.0.0",
    "nock": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "open": "^8.4.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.66.0",
    "webpack-clean-plugin": "^0.2.3",
    "webpack-cli": "^4.9.1",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-hot-middleware": "^2.25.1"
  }
}
